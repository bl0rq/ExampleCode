<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:Control="using:Utilis.UI.Control">
	<Style
		TargetType="Control:HeaderedContentControl">
		<Setter
			Property="Template">
			<Setter.Value>
				<ControlTemplate
					TargetType="Control:HeaderedContentControl">
					<StackPanel>
						<ContentPresenter
							Content="{TemplateBinding Header}" />
						<ContentPresenter
							Content="{TemplateBinding Content}" />
					</StackPanel>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
</ResourceDictionary>
